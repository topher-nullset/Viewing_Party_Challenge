<h2>Log In Page</h2>

<section id="login-form">
  <%= form_with url: login_path, method: :post do |form| %>
    <%= form.label :email, "Email:" %>
    <%= form.text_field :email %>
    <%= form.label :password, "Password:" %>
    <%= form.text_field :password %>
    <%= form.submit "Log In" %>
  <% end %>
</section>